{
    "endpoints": [
      {
        "name": "get_top_tracks",
        "method": "GET",
        "path": "/v1/tracks/top",
        "query_params": {
          "days": [7, 30, 90],
          "limit": [10, 50]
        }
      },
      {
        "name": "create_user",
        "method": "POST",
        "path": "/v1/users",
        "body": {
          "name": "loadtest_user_{{randomInt(1000,9999)}}",
          "email": "test_{{randomString(8)}}@example.com"
        }
      }
    ]
  }